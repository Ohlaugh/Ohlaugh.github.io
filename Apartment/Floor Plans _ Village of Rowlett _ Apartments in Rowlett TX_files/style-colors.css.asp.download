    color: #ffffff
}

#uni-header.compact #main-nav li:hover, #uni-header.force-compact #main-nav li:hover {
	background:#ffffff;
}
#uni-header.compact #main-nav li:hover a span, #uni-header.force-compact #main-nav li:hover a span {
    color: #9F0101
}

#uni-header.compact #main-nav {
	background:#9F0101;
}

#uni-header.force-compact #main-nav {
	background:#9F0101 !important;
}


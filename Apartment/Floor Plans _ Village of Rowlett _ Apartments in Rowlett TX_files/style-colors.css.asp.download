/*
402
element >> default >> selected
PrimaryBG >> #9F0101 >> #9F0101
PrimaryFG >> #ffffff >> #ffffff
RolloverBG >> #ffffff >> #ffffff
RolloverFG >> #9F0101 >> #9F0101
HeaderFG >> #a60202 >> #a60202
body >> Source Sans >> Montserrat-Regular-webfont
overlay >> DISCO___-webfont >> Montserrat-Regular-webfont
*/

body {font-family: "Montserrat-Regular-webfont", "Source Sans"}
/* Montserrat-Regular-webfont - 94% */body{font-size: 94% !important;} 
h1, h2, h3, h4 {font-family: "Montserrat-Regular-webfont", "Source Sans"} 

div.tagline span {font-family: "Montserrat-Regular-webfont", "DISCO___-webfont"}

h2, #preview-center-banner > div.content, .article-card__title {color: #a60202}
#preview-center-banner > div.content {font-weight:bold;}

a, #page-reviews div.overall a { 
	color: #a60202
}

#preview-center-banner > div.content {color: #fff;}
#preview-center-banner > div.content  {font-family: Montserrat-Regular-webfont}
#preview-center-banner{background: #9F0101; font-family: Montserrat-Regular-webfont}
.spotItemContainer{font-family: Montserrat-Regular-webfont}
.spotlightP a {background: #9F0101;}

.site-section.accented2 form button, div.btns input[type="radio"]:checked + span {
	background:#9F0101 !important;
    color:#ffffff !important;
}

.help-bg, .help-bg-hover {
	background-color: #9F0101 !important;
    color: #ffffff !important;
}

.details__loader path, .details__loader rect{
	fill: #9F0101;
}

.help-bg-hover:hover {
	color: #9F0101 !important;
    background-color: #ffffff !important;
}

.time-container input[type="radio"]:checked ~ .radio {
	background: #9F0101;
	border-color: #9F0101;
}

.is-today .pika-button {
	color: #9F0101 !important;
}
.is-selected .pika-button {
	color: #fff !important;
	background: #9F0101 !important;
}
.plan-number.selected {
	color: #fff !important;
	background: #9F0101 !important;
}

#main-nav li a span {
	color:#fff;
}

#main-nav li.active a,  {
	color:#ffffff;
}
#main-nav li.active a {
	background:#9F0101;
}


#main-nav li.subnav ul a span {color:white;}
#main-nav li.subnav ul li:hover a span {color:#9F0101;}


#uni-slideshow .slides li:before {
	background:url('/images/overlays/blank.png') rgba(0,0,0,.65);
	opacity:0;
}

.ie8 #uni-slideshow .slides li:before {
	background:url('/images/overlays/blank.png') transparent;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(opacity=65)";
	filter:"progid:DXImageTransform.Microsoft.Alpha(opacity=65)";
}

#main-nav .subnav:after {
    border-left:6px solid transparent;
    border-right:6px solid transparent;
    border-top:6px solid #ffffff;
}

#uni-header.compact #main-nav .subnav:after, #uni-header.force-compact #main-nav .subnav:after {
    border-left:6px solid transparent;
    border-right:6px solid transparent;
    border-top:6px solid #ffffff;
}

#main-nav > ul > li.open,
#main-nav > ul > li.open > ul > li:hover {
	color:#9F0101;
    background: #ffffff;
}

#main-nav > ul > li.open > a,
#main-nav > ul > li.open > ul > li:hover > a {
	color:#9F0101;
    background: #ffffff;
}


#main-nav .subnav.open:after {
    border-left:6px solid transparent;
    border-right:6px solid transparent;
    border-top:6px solid #9F0101;
}

#main-nav ul > li.active, #main-nav ul > li.active:hover {
	background:#9F0101; /* Old browsers */
	background:-moz-linear-gradient(top, #bf0202 0%, #9F0101 100%); /* FF3.6+ */
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#bf0202), color-stop(100%,#9F0101)); /* Chrome,Safari4+ */
	background:-webkit-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* Chrome10+,Safari5.1+ */
	background:-o-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* Opera 11.10+ */
	background:-ms-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* IE10+ */
	background:linear-gradient(to bottom, #bf0202 0%,#9F0101 100%); /* W3C */
}

/*
#main-nav ul > li:hover > a, #main-nav ul > li.active > a {
	background:url('/images/overlays/blank.png');
}
*/


#uni-header.compact #main-nav > ul > li.subnav.open:after, #uni-header.compact #main-nav ul > li.subnav.active:after,
#uni-header.force-compact #main-nav > ul > li.subnav.open:after, #uni-header.force-compact #main-nav ul > li.subnav.active:after {
	border-left:6px solid transparent;
	border-right:6px solid transparent;
	border-top:6px solid #9F0101;
}

#uni-header.compact #main-nav > ul > li.open > a, #uni-header.compact #main-nav ul > li.active > a,
#uni-header.force-compact #main-nav > ul > li.open > a, #uni-header.force-compact #main-nav ul > li.active > a {
	background:none;
	color:#9F0101;
}

#menu-button {
	background:#ffffff;
}

#menu-button:before {
	border-bottom: 13px double #4c4c4c;
	border-top: 4px solid #4c4c4c;
}

#main-nav .subnav ul {
	background:url('/images/overlays/blank.png') rgba(0,0,0,.80);
	border:1px solid #000000;
}

@media screen and (max-width:48em){ /*768px*/
	#main-nav ul > li:hover, #main-nav ul > li.active {
		background:none;
	}
	
	#uni-header.compact #main-nav > ul > li.active > a, #uni-header.force-compact #main-nav > ul > li.active > a, #main-nav > ul > li.active > a, #main-nav > ul > li.subnav.opened > a {
		background:#9F0101; /* Old browsers */
		background:-moz-linear-gradient(top, #bf0202 0%, #9F0101 100%); /* FF3.6+ */
		background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#bf0202), color-stop(100%,#9F0101)); /* Chrome,Safari4+ */
		background:-webkit-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* Chrome10+,Safari5.1+ */
		background:-o-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* Opera 11.10+ */
		background:-ms-linear-gradient(top, #bf0202 0%,#9F0101 100%); /* IE10+ */
		background:linear-gradient(to bottom, #bf0202 0%,#9F0101 100%); /* W3C */
		color:#ffffff;
	}
	
	#main-nav > ul > li.open > a, #main-nav > ul > .subnav > ul > li.open > a {
		background:#ffffff;
		color:#9F0101;
	}
	
	#uni-header.compact #main-nav > ul > li.open, #uni-header.compact #main-nav ul > li.active, #uni-header.force-compact #main-nav > ul > li.open, #uni-header.force-compact #main-nav ul > li.active {
		background:none;
	}
	
	#uni-header.compact #main-nav > ul > li.open > a, #uni-header.force-compact #main-nav > ul > li.open > a {
		background:#ffffff;
	}
	
	#main-nav > ul > li.subnav.open:after {
		border-left:6px solid transparent;
		border-right:6px solid transparent;
		border-top:6px solid #9F0101;
	}
	
	#uni-header.compact #main-nav > ul > li.subnav.opened:after, #uni-header.force-compact #main-nav > ul > li.subnav.opened:after, #main-nav > ul > li.subnav.opened:after {
		border-left:6px solid transparent;
		border-right:6px solid transparent;
		border-top:6px solid transparent;
		border-bottom:6px solid #9F0101;
		top:30px;
	}
	
	#uni-header.compact #main-nav > ul > li.subnav.opened:hover:after, #uni-header.force-compact #main-nav > ul > li.subnav.opened:hover:after, #main-nav > ul > li.subnav.opened:hover:after {
		border-left:6px solid transparent;
		border-right:6px solid transparent;
		border-top:6px solid transparent;
		border-bottom:6px solid #ffffff;
		top:30px;
	}
	
	#main-nav .subnav ul {
		background:#303030;
		border:none;
	}
	
	#main-nav > ul > li.subnav.open:after, #main-nav ul > li.subnav.active:after {
		border-left:6px solid transparent;
		border-right:6px solid transparent;
		border-top:6px solid transparent;
		border-bottom:6px solid #9F0101;
		top:30px;
	}
	
	#uni-header.compact #main-nav > ul > li.subnav.open:after, #uni-header.compact #main-nav ul > li.subnav.active:after, #uni-header.force-compact #main-nav > ul > li.subnav.open:after, #uni-header.force-compact #main-nav ul > li.subnav.active:after {
		border-left:6px solid transparent;
		border-right:6px solid transparent;
		border-top:6px solid transparent;
		border-bottom:6px solid #9F0101;
		top:30px;
	}
}


